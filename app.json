{
  "expo": {
    "name": "expo-zesty",
    "slug": "expo-zesty",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "expozesty",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.anonymous.expozesty"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.expozesty"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#E6F4FE"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/DINPro.otf",
            "./assets/fonts/DINPro-Light.otf",
            "./assets/fonts/DINPro-Medium.otf",
            "./assets/fonts/DINPro-Bold.otf",
            "./assets/fonts/DINPro-Black.otf",
            "./assets/fonts/FF_DIN_Pro_Regular_Italic.otf",
            "./assets/fonts/FF_DIN_Pro_Light_Italic.otf",
            "./assets/fonts/FF_DIN_Pro_Medium_Italic.otf",
            "./assets/fonts/FF_DIN_Pro_Bold_Italic.otf",
            "./assets/fonts/FF_DIN_Pro_Black_Italic.otf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
